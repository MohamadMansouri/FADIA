        <input pattern="results/DCFnofrag-#0.vec"/>
        <input pattern="results/HCFfrag-#0.sca"/>
        <input pattern="results/HCFfrag-#0.vec"/>
        <input pattern="results/HCFfragblockack-#0.sca"/>
        <input pattern="results/HCFfragblockack-#0.vec"/>
        <input pattern="results/OnlyPacketSize*.sca"/>
        <input pattern="results/OnlyPacketSize*.vec"/>
        <input pattern="results/Threshold*.sca"/>
        <input pattern="results/Threshold*.vec"/>
        <input pattern="results/PacketSize*.sca"/>
        <input pattern="results/PacketSize*.vec"/>
        <input pattern="results/TxPower*.vec"/>
        <input pattern="results/TxPower*.sca"/>
    </inputs>
    <charts>
        <chart icon="icons/full/obj/barchart.png" id="1393" name="Number of packets received" template="barchart_mpl" type="MATPLOTLIB">
            <script><![CDATA[import numpy as np
import pandas as pd
from omnetpp.scave import results, chart, ideplot, utils
import matplotlib.pyplot as plt

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]

# query scalar data into dataframe
df = results.get_scalars(filter_expression, include_attrs=True, include_itervars=True, include_runattrs=True)

if df.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

groups = props["groups"].split(",")
series = props["series"].split(",")

if not groups[0] and not series[0]:
    print("The Groups and Series options were not set in the dialog, inferring them from the data.")
    g, s = ("module", "name") if len(df) == 1 else utils.select_best_partitioning_column_pair(df, props)
    if not g or not s:
        raise chart.ChartScriptError("Please set the Groups and Series options in the dialog!")
    groups, series = [g], [s]

utils.assert_columns_exist(df, groups + series)

for c in groups + series:
    df[c] = pd.to_numeric(df[c], errors="ignore")

#df.sort_values(by=groups+series, axis='index', inplace=True)

df = df.set_index('configname')
df = df.reindex(['DCFnofrag','DCFfrag','HCFfrag','HCFfragblockack'])

df.plot(kind='bar', ax=plt.gca(), zorder=2, align='center', width=0.8, figsize=(10,6))
plt.grid(axis='y', linestyle='--')
plt.grid(axis='x', linestyle='')

#confidence_level_str = props["confidence_level"] if "confidence_level" in props else "none"
#
#if confidence_level_str == "none":
#    df = pd.pivot_table(df, index=groups, columns=series, values='value')
#    utils.plot_bars(df, props, names)
#else:
#    confidence_level = float(confidence_level_str[:-1])/100
#    conf_int = lambda values: utils.confidence_interval(confidence_level, values) if len(values) > 1 else math.nan
#    df = pd.pivot_table(df, index=groups, columns=series, values='value', aggfunc=[np.mean, conf_int], dropna=False)
#    utils.plot_bars(df["mean"], props, names, df["<lambda>"])

utils.postconfigure_plot(props)

plt.xlabel('')
plt.xticks(rotation=0)
plt.title('Number of packets received, 2000-byte packets and 250-byte fragmentation threshold')

ax = plt.gca()

i = 0
for p in ax.patches:
    if(i):
        ax.annotate(str(int(p.get_height())), (p.get_x() + p.get_width() / 2, p.get_height() - 100),
                    horizontalalignment='center', verticalalignment='top', color='white', size=12)
    else:
        ax.annotate(str(int(p.get_height())), (p.get_x() + p.get_width() / 2, p.get_height() + 200),
                    horizontalalignment='center', verticalalignment='top', color='black', size=12)
    i += 1
    
ax.legend().remove()

utils.export_image_if_needed(props)
utils.export_data_if_needed(df, props)
]]></script>
