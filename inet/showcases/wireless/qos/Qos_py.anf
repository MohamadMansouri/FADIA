        <chart icon="icons/full/obj/linechart.png" id="1537" name="Throughput, non-QoS" template="linechart_mpl" type="MATPLOTLIB">
            <script><![CDATA[import math
from omnetpp.scave import results, chart, utils, ideplot, vectorops as ops
import matplotlib.pyplot as plt
import getAc as ac

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]
start_time = float(props["vector_start_time"] or -math.inf)
end_time = float(props["vector_end_time"] or math.inf)

# query vector data into a data frame
try:
    df = results.get_vectors(filter_expression, include_attrs=True, include_itervars=True, start_time=start_time, end_time=end_time)
except ValueError as e:
    raise chart.ChartScriptError("Error while querying results: " + str(e))

if df.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

# apply vector operations
df = ops.perform_vector_ops(df, props["vector_operations"])
df.sort_values("module", inplace=True)

for i in range(0,len(df['module'])):
    df['module'][i] = ac.get_AC(df['module'][i])

# plot
utils.plot_vectors(df, props)

plt.grid(linestyle='--')
#plt.tight_layout(1.5)

utils.postconfigure_plot(props)

# TODO this should be set in the dialog
plt.title('Throughput, non-QoS')

utils.export_image_if_needed(props)
utils.export_data_if_needed(df, props)

plt.legend(markerscale=4)]]></script>
